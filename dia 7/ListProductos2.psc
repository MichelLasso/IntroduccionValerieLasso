Algoritmo ListProductos 
	dimension prod[3] 
	prod[1]="fresa" 
	prod[2]="mora"
	prod[3]="manzana" 
	para i<-1 hasta 3 hacer 
		escribir prod[i],;
	FinPara
	dimension precio[3]
	precio[1]="500"
	precio[2]="700"
	precio[3]="2000"
	para i<-1 hasta 3 hacer 
		escribir precio[i],;
	FinPara
	
FinAlgoritmo
